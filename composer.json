{
  "name": "deathnerd/php-wtforms",
  "description": "A PHP rewrite of the fantastic Python library WTForms",
  "minimum-stability": "dev",
  "license": "GPL-2.0",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/deathnerd/php-wtforms.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/deathnerd/php-annotations.git"
    }
  ],
  "authors": [
    {
      "name": "Wes Gilleland",
      "email": "wes.gilleland@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "WTForms\\": "src/",
      "WTForms\\CSRF\\": "src/csrf/",
      "WTForms\\Fields\\": "src/fields/",
      "WTForms\\Fields\\Core\\": "src/fields/core/",
      "WTForms\\Fields\\Core\\Annotations\\": "src/fields/core/annotations/",
      "WTForms\\Fields\\HTML5\\": "src/fields/html5/",
      "WTForms\\Fields\\HTML5\\Annotations\\": "src/fields/html5/annotations/",
      "WTForms\\Fields\\Simple\\": "src/fields/simple/",
      "WTForms\\Fields\\Simple\\Annotations\\": "src/fields/simple/annotations/",
      "WTForms\\Widgets\\": "src/widgets/",
      "WTForms\\Widgets\\Core\\": "src/widgets/core",
      "WTForms\\Widgets\\HTML5\\": "src/widgets/html5",
      "WTForms\\Validators\\": "src/validators/",
      "WTForms\\Validators\\Annotations\\": "src/validators/annotations/"
    },
    "files": ["src/helpers.php"]
  },
  "autoload-dev": {
    "psr-4":{
      "WTForms\\Tests\\": "tests/",
      "WTForms\\Tests\\Common\\": "tests/common/",
      "WTForms\\Tests\\Fields\\": "tests/fields/",
      "WTForms\\Tests\\SupportingClasses\\": "tests/supporting_classes/"
    }
  },
  "require": {
    "php": ">=5.4",
    "mindplay/annotations": "1.3.0"
  },
  "require-dev": {
    "filp/whoops": "^2.1",
    "maximebf/debugbar": "1.*"
  }
}
